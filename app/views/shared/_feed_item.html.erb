<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
  <span class="user">
    <%= link_to feed_item.user.name, feed_item.user %>
  </span>
  <span class="content"><%= feed_item.title%></span>
    <p><span class="title">Room description:</span><%= feed_item.room_description %>
    <p><span class="title">Include breakfast:</span><%= feed_item.include_breakfast %>
    <p><span class="title">Price:</span> <%= feed_item.price %>
    <p><span class="title">Adress:</span> <%= feed_item.adress %></span>
      <p><span class="title">Stars:</span> <%= feed_item.star_rate_hotel %></span>
      <p><%= image_tag feed_item.photo.url%>
  <p><span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at)%> ago.
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago. Stars : <%= rating_for feed_item, "stars", :star => 5 %>
  </span>
  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete, data: {confirm: "You sure?"},
                                                                                        title: feed_item.title %>
      <% end %>
</li>